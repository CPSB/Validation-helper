{
    "name": "activismebe/laravel-form-helpers", 
    "description": "Handle form model binding, old input binding and validation error messages in a clean easy way.",
    "keywords": ["laravel", "form", "html", "eloquent", "input", "validation", "session", "helpers"],
    "license": "MIT", 
    "authors": [
        {
            "name": "Tim Joosten",
            "email": "Tim@activisme.be"
        }
    ], 
    "require": {
        "php": ">=5.5.9"
    },
    "autoload": {
        "psr-4": {
            "ActivismeBE\\FormHelper\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "laravel/laravel": "~5.5"
    },
    "extra": {
        "laravel" : {
            "providers": [
                "ActivismeBE\\FormHelper\\FormServiceProvider"
            ]
        }
    }
}